<h1>Listing interfaces</h1>
<div class ="dataindex">
<table>
  <thead>
    <tr>
      <th>Api version</th>
      <th>Api ref</th>
      <th>Api name</th>
      <th>Api type</th>
      <th>Ymlname</th>
      <th>URL</th>
      <th>Description </th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @interfaces.each do |interface| %>
      <tr>
        <td><%= interface.api_version %></td>
        <td><%= interface.api_ref %></td>
        <td><%= interface.api_name %></td>
        <td><%= interface.api_type %></td>
        <td><%= interface.ymlname %></td>
        <td><a href="<%= interface.url %>" target="_new"><%= interface.url %> </a> </td>
        <td><%= interface.short_description %> </td>
        <td><%= link_to 'Show', interface %></td>
        <td><%= link_to 'Edit', edit_interface_path(interface) %></td>
        <td><%= link_to 'Destroy', interface, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<br>

<%= link_to 'New Interface', new_interface_path %>
